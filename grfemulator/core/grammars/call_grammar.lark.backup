start: [call]*
?func: name
    | i
    | const
call: func "(" [INT ("," INT)*]  ")"
name: CNAME
i: "I" "^" INT "_" INT
const: INT "^" INT


%import common.CNAME
%import common.INT
%import common.WS
%import common.SH_COMMENT
%import common.C_COMMENT
%ignore SH_COMMENT         // Comment of the form # ... (to end of line)
%ignore C_COMMENT          // Comment of the form /* ... */
%ignore WS


